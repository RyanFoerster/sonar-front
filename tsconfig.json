{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": [
      "ES2022",
      "dom"
    ],
    "paths": {
      "@spartan-ng/ui-accordion-helm": [
        "./components/ui-accordion-helm/src/index.ts"
      ],
      "@spartan-ng/ui-alert-helm": [
        "./components/ui-alert-helm/src/index.ts"
      ],
      "@spartan-ng/ui-alertdialog-helm": [
        "./components/ui-alertdialog-helm/src/index.ts"
      ],
      "@spartan-ng/ui-aspectratio-helm": [
        "./components/ui-aspectratio-helm/src/index.ts"
      ],
      "@spartan-ng/ui-button-helm": [
        "./components/ui-button-helm/src/index.ts"
      ],
      "@spartan-ng/ui-dialog-helm": [
        "./components/ui-dialog-helm/src/index.ts"
      ],
      "@spartan-ng/ui-icon-helm": [
        "./components/ui-icon-helm/src/index.ts"
      ],
      "@spartan-ng/ui-input-helm": [
        "./components/ui-input-helm/src/index.ts"
      ],
      "@spartan-ng/ui-label-helm": [
        "./components/ui-label-helm/src/index.ts"
      ],
      "@spartan-ng/ui-menu-helm": [
        "./components/ui-menu-helm/src/index.ts"
      ],
      "@spartan-ng/ui-popover-helm": [
        "./components/ui-popover-helm/src/index.ts"
      ],
      "@spartan-ng/ui-separator-helm": [
        "./components/ui-separator-helm/src/index.ts"
      ],
      "@spartan-ng/ui-table-helm": [
        "./components/ui-table-helm/src/index.ts"
      ],
      "@spartan-ng/ui-checkbox-helm": [
        "./components/ui-checkbox-helm/src/index.ts"
      ],
      "@spartan-ng/ui-select-helm": [
        "./components/ui-select-helm/src/index.ts"
      ],
      "@spartan-ng/ui-helm-helm": [
        "./components/ui-helm-helm/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
